from itertools import permutations
from collections import defaultdict
def solution(expression):
    check = []
    num = []
    numset = []
    temp = ""
    for i in expression:
        if not i.isdigit():
            check.append(i)
            if not i in numset:
                numset.append(i)
            num.append(temp)
            temp = ""
        else:
            temp += i
    num.append(temp)
    
    perm = list(permutations(numset))
    
    print(check)

    for i in range(len(perm)):
        answer = max(circle(perm[i],check,num),0)
    
