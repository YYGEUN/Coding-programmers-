    return answer

def circle(perm,check,num):
    print(num)
    for i in perm:
        while i in check:
            if i == "-":
                num[check.index(i)] = str(int(num[check.index(i)]) - int(num[check.index(i)+1]))
                del num[check.index(i)+1]
            elif i == "+":
                num[check.index(i)] = str(int(num[check.index(i)]) + int(num[check.index(i)+1]))
                del num[check.index(i)+1]
            else:
                num[check.index(i)] = str(int(num[check.index(i)]) * int(num[check.index(i)+1]))
                del num[check.index(i)+1]
            check.remove(i)
    print(int(num[0]),num)
            
    return abs(int(num[0]))
    #### 변하지않는 변수 지정 방법 찾기 필요
